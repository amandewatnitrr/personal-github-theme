{
    "activityBarBadge.background": "#000a41",
    "activityBar.inactiveForeground": "#ffffff",
    "activityBar.foreground": "#00cfff",

    "editor.suggestSelection": "first",
    "workbench.iconTheme": "material-icon-theme",
    "code-runner.runInTerminal": true,
    "prettier.requirePragma": true,
    "prettier.useTabs": true,
    "editor.minimap.enabled": false,
    "window.titleBarStyle": "custom",
    "tabnine.experimentalAutoImports": true,
    "editor.fontFamily": "'Cascadia Code',Consolas, 'Courier New', monospace",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": true
    },
    "git-graph.maxDepthOfRepoSearch": 1000,
    "editor.guides.bracketPairs": true,
    "vsintellicode.features.apiExamplests": "enabled",
    "vsintellicode.features.python.deepLearning": "enabled",
    "vsintellicode.features.apiExamples": "enabled",
    "http.proxyAuthorization": null,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Ubuntu-16.04 (WSL)": {
            "path": "C:\\windows\\System32\\wsl.exe",
            "args": [
                "-d",
                "Ubuntu-16.04"
            ]
        }
    },
    "vscode-pets.petSize": "large",
    "vscode-pets.theme": "forest",
    "terminal.integrated.gpuAcceleration": "off",
    "vscode_vibrancy.theme": "Dark (Exclude Tab Line)",
    "vscode_vibrancy.type": "ultra-dark",
    "cSpell.userWords": [
        "unsupportive"
    ],
    "git.enableSmartCommit": true,
    "sonarlint.rules": {
        "java:S2095": {
            "level": "off"
        },
        "java:S106": {
            "level": "off"
        },
        "java:S2189": {
            "level": "off"
        }
    },
    "editor.fontSize": 16,
    "workbench.colorTheme": "Bearded Theme Monokai Black",
    "editor.fontWeight": "bold",
    "comments": "#00ffdb",
    "workbench.colorCustomizations": {
        // Customized
        "activityBar.inactiveForeground": "#ffffff",
        "activityBar.foreground": "#00cfff",
        "editor.background": "#000000",
        "editor.selectionBackground": "#264f78",
        "editorLineNumber.activeForeground": "#4d91f7",
        "editorLineNumber.foreground": "#f5f5f5",
        "editor.findMatchBackground": "#515c6a",
        "editor.findMatchBorder": "#74879f",
        "editor.findMatchHighlightBackground": "#ea5c0055",
        "editorSuggestWidget.background": "#0c0c24",
        "editor.foreground":"#ffffff",
        "sideBar.background": "#000000",
        "sideBar.foreground": "#ffffff",
        "editorRuler.foreground": "#5a5a5a",
        "comments": "#00ffdb",
        "activityBarBadge.background": "#000a41",
        "statusBar.background": "#007acc",
        "statusBarItem.remoteBackground": "#16825d",
    },
    "editor.tokenColorCustomizations": {
        "comments": "#02f2ae85",
        
    },
}
